<% if game.errors.any? %>
  <div style="color: red">
    <% game.errors.each do |error| %>
      <%= error.full_message %>
    <% end %>
  </div>
<% end %>
<%= form_with(model: game) do |form| %>
  <%= form.label :game_date %>
  <%= form.date_select :game_date %>
  <%= form.collection_check_boxes :participant_ids, Participant.all, :id, :name,   include_hidden: false %>
  <%= form.collection_check_boxes :snack_ids, Snack.all, :id, :name, include_hidden: false %>
  <%= form.submit %>
<% end %>



